# Binaries
[binaries]
c = ['clang', '-m32']
c_ld = 'lld'   # Must have llvm installed to use
#ld = 'ld'     # Optional instead of llvm
ar = 'ar'
strip = 'strip'

# Properties
[properties]
c_args = ['-ffreestanding']
c_link_args = ['-nostdlib']

# Host Machinne 
[host_machine]
system = 'ibox-kernel'
cpu_family = 'x86'
cpu = 'i686'
endian = 'little'
