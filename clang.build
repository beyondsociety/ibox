[binaries]
c = 'clang'
c_ld = 'lld'
ar = 'ar'
strip = 'strip'

# Not used
pkconfig = ''
exe_wrapper = ''

[properties]
has_function_printf = 'false'
needs_exe_wrapper = 'false'

project_configuration = [
	'b_as_needed=false',
	'b_no_undefined=false',
  	'buildtype=plain',
  	'c_std=c99' ]

[host_machine]
system = 'ibox-kernel'
cpu_family = 'x86'
cpu = 'i686'
endian = 'little'
