[binaries]
c = 'i686-elf-gcc'
ld = 'i686-elf-ld'
ar = 'i686-elf-ar'
strip = 'i686-elf-strip'

# Not used
pkgconfig = ''
exe_wrapper = ''

[properties]
#c_args = '-ffreestanding'
#link_args = '-nostdlib'

has_function_printf = 'false'
needs_exe_wrapper = 'false'

project_configuration = [
	'b_asneeded=false',
	'b_lundef=false',
	'buildtype=plain',
	'c_std=c99' ]

[host_machine]
system = 'ibox-kernel'
cpu_family = 'x86'
cpu = 'i686'
endian = 'little'
