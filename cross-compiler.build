[binaries]
c = 'i686-elf-gcc'
ld = 'i686-elf-ld'
ar = 'i686-elf-ar'
strip = 'i686-elf-strip'

[properties]
c_args = ['-ffreestanding']
c_link_args = ['-ffreestanding', '-nostdlib']

project_configuration = [
	'b_asneeded=false',
	'b_lundef=false',
	'buildtype=plain',
	'c_std=c99' ]

[host_machine]
system = 'ibox-kernel'
cpu_family = 'x86'
cpu = 'i686'
endian = 'little'
